%h1 Add New Restaurant
= form_for @restaurant do |f|
  = f.label :name
  = f.text_field :name

  = f.label :address
  = f.text_field :address

  = f.label :city
  = f.text_field :city

  = f.label :zipcode
  = f.text_field :zipcode

  = f.label :description
  = f.text_field :description

  = f.label :catagory
  = f.text_field :catagory

  = f.submit "Add Restaurant"
